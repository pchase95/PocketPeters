import java.awt.*;
import java.awt.event.*;
import javax.swing.*;

public class Map extends JPanel
{
	private JFrame f;
	
	public Map()
	{
		f = new JFrame("Map");
		f.setSize(800,600);
		f.setDefaultCloseOperation(JFrame.EXIT_ON_CLOSE);
		this.setBackground(Color.RED);
		
		
		f.add(this);
		f.setVisible(true);
	}
}
