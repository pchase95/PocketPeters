import java.util.*;

public class Game
{
	private Scanner keys;
	public static void main(String[] args)
	{
		new Game();
	}
	
	public Game()
	{
		keys = new Scanner(System.in);
		init();
	}
	
	private void init()
	{
		while(true)
		{
			
		}
	}
	
	
	private String getInput(String input)
	{
		input = keys.nextLine();
		return input;
	}
}
