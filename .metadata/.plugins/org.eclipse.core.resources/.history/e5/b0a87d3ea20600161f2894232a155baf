
public class Packet00Login extends Packet {

	private String username;
	
	public Packet00Login(byte[] data) {
		super(00);
		this.username = readData(data);
	}

	@Override
	public void writeData(DatagramClient client) {
		
	}

	@Override
	public void writeData(DatagramServer server) {
		
	}

	@Override
	public byte[] getData() {
		return null;
	}

}
